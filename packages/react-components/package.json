{
  "name": "@puppet/react-components",
  "version": "5.16.0",
  "author": "Puppet, Inc.",
  "license": "Apache-2.0",
  "main": "build/library.js",
  "scripts": {
    "link": "npm link",
    "analyze": "webpack --config webpack.analyze.config.js",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepare": "npm run build",
    "complexity": "es6-plato -r -d complexity source/react && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "nyc --require=./test/jsdom.js --reporter=html mocha --require @babel/register --require core-js --require regenerator-runtime/runtime --recursive",
    "lint": "npm run js-lint && npm run scss-lint",
    "js-lint": "eslint --quiet 'source/react/**/*.js'",
    "scss-lint": "stylelint 'source/scss/**/*.scss'",
    "format": "eslint --fix 'source/**/*.{js,jsx}'; prettier --write 'source/**/*.{js,jsx,json,md}'",
    "start": "cd ../design-system-website && npm start"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "es6-plato": "^1.2.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-csv": "^3.4.0",
    "file-loader": "^4.2.0",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.2",
    "mocha-jsdom": "^2.0.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "react-addons-test-utils": "^15.6.2",
    "react-router": "^5.1.2",
    "react-styleguidist": "^10.2.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@puppet/sass-variables": "^1.4.0",
    "classnames": "^2.2.6",
    "core-js": "^3.4.1",
    "hoist-non-react-statics": "^3.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-transition-group": "^4.3.0",
    "regenerator-runtime": "^0.13.3",
    "scroll-into-view-if-needed": "^2.2.20"
  },
  "gitHead": "4a6c41ff75aabb2ec705588e6ab757fc69ac9f97"
}
